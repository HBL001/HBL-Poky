# Custom WIC layout for BeagleBone Black using extlinux bootloader

part /boot --source bootimg-partition \
           --sourceparams="loader=extlinux" \
           --ondisk mmcblk0 \
           --fstype=vfat \
           --label boot \
           --active \
           --align 8192

part / --source rootfs \
       --ondisk mmcblk0 \
       --fstype=ext4 \
       --label rootfs \
       --align 8192

bootloader --ptable msdos \
           --timeout=5 \
           --append="${APPEND}"

